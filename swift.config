sites: [localhost]

site.localhost {
    execution {
        type: "local"
        URL : "localhost"
    }
    staging             : local
    workDirectory       : "../Variant-Calling-Data/tmp/Swift"
    maxParallelTasks    : 20
    initialParallelTasks: 20
    app.ALL { executable: "*" }
}

TCPPortRange: "50000,51000"
lazyErrors: false
executionRetries: 4
keepSiteDir: true
providerStagingPinSwiftFiles: false
alwaysTransferWrapperLog: true
